<h3 class="title"><%=l(:label_version_new)%></h3>

<%= labelled_form_for @version, :url => project_versions_path(@project), :remote => true do |f| %>

<div class="versionPop">

<%= render :partial => 'versions/form', :locals => { :f => f } %>
  <p class="buttons">
    <%= submit_tag l(:button_create), :name => nil, :class => 'greenBtn' %>
    <%= submit_tag l(:button_cancel), :name => nil, :class => 'blueBtn', :onclick => "hideModal(this);", :type => 'button' %>
  </p>

</div>


<% end %>

<script type="text/javascript">
$("select").selectbox();
</script>
