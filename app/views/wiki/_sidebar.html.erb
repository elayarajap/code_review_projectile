<aside class="contentRightColumn">
	<% if @wiki && @wiki.sidebar -%>
	  <%= textilizable @wiki.sidebar.content, :text %>
	<% end -%>

	<h3><%= l(:label_wiki) %></h3>
	<ul>
		<li><%= link_to l(:field_start_page), {:action => 'show', :id => nil},:class=>'startPage' %></li>
		<li><%= link_to l(:label_index_by_title), {:action => 'index'},:class=>'indexByTitle' %></li>
		<li><%= link_to l(:label_index_by_date), {:controller => 'wiki', :project_id => @project, :action => 'date_index'},:class=>'indexByDate' %></li>
	</ul>	
</aside>